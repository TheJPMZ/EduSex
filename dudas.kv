<Dudas>:
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        BoxLayout:
			orientation: 'horizontal'
			size_hint_y: .1
			spacing: 10
            MDIconButton:
                icon: 'less-than'
                size_y: 1
                text_color: 0,0,0,1
				on_press: root.manager.current = 'menu'
			MDTextField:
				hint_text: 'Buscar'
				height: "10dp"
				width: "10dp"
				mode: 'rectangle'
				line_color_focus: 0,0,1,1
				on_text: root.set_list(self.text,True)
            MDIconButton:
                icon: 'magnify'
                size_y: 1
        ScrollView:
            BoxLayout:
                size_hint_y: None
                height: 600
                orientation: 'vertical'
                padding: 8, 0 , 8 , 8
                margin: 9
                id: meme
                Button:
                    size_hint: 1, None
                    size_y: 100
                    text: "memes"
                Button:
                    size_hint: 1, None
                    size_y: 100
                    text: "memes"

