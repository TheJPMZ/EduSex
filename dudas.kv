<Dudas>:
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        BoxLayout:
			orientation: 'horizontal'
			size_hint_y: .1
			spacing: 10
            MDIconButton:
                icon: 'less-than'
                size_y: 1
                text_color: 0,0,0,1
				on_press: root.manager.current = 'menu'
			MDTextField:
				hint_text: 'Buscar'
				height: "10dp"
				width: "10dp"
				mode: 'rectangle'
				line_color_focus: 0,0,1,1
				on_text: root.set_list(self.text,True)
            MDIconButton:
                icon: 'magnify'
                size_y: 1

        BoxLayout:
            orientation:'horizontal'
            
            ScrollView:
                size_hint: 1, 0.8
                StackLayout:
           
                    adaptive_height: True
                    orientation: 'lr-tb'
                    padding: 25,0,25,0
                    size_hint: 1, 2
                    id: sexo
                    spacing: 5

        ScrollView:
            MDBoxLayout:
                adaptive_height: True
                orientation: 'vertical'
                padding: 8, 0 , 8 , 8
                margin: 9
                id: meme
                spacing: 5
                Button:
                    size_hint: 1, None
                    size_y: 6
                    